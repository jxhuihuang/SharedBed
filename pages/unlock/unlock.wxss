page {
    background-color: #fff;
}

.container {
    padding: 1px 0px;
    background: #fff;
    display: block;
    justify-content: flex-start;
    text-align: center;
    overflow: hidden;
}

.state-container {
    color: rgb(42,200,179);
    margin-top: 10px;
}

.unlock-container {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    box-sizing: border-box;
    background: #fff;
    box-shadow: 0 0 12px #ddd;
    padding-bottom: 16px;
}

.banner {
    display: flex;
    justify-content: center;
    align-content: center;
    flex-wrap: wrap;
    position: relative;
}

.banner image {
    display: block;
    width: 288px;
    height: 135px;
}

.qrcode-scan-entry {
    width: 48px;
    height: 48px;
    background-color: #f7f7f7;
    border-radius: 50%;
    right: 3px;
    position: absolute;
    top: 3px;
}

