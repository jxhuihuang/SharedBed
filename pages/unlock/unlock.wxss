.qrcode-scan-entry::after {
    content: '扫码用车';
    display: block;
    width: 48px;
    height: 20px;
    text-align: center;
    color: #333;
    font-size: 10px;
    transform: scale(0.7);
}

.charge {
    font-size: 12px;
    color: #40320d;
    background: #f7f7f7;
    border-radius: 100px;
    padding: 5px 15px;
    margin-bottom: 10px;
}

.form-content {
    display: flex;
    padding: 0 16px;
    flex-flow: row no-wrap;
    justify-content: space-between;
}

.input-num-wrapper {
    height: 58px;
    border: 1px solid #fed52f;
    flex: 1;
}

.form-sub {
    width: 60px;
    height: 60px;
    margin-left: 10px;
}

.form-sub view {
    width: 100%;
    height: 100%;
}

.form-sub image {
    display: block;
    width: 100%;
    height: 100%;
}

.warnMsg {
    color: #f00;
    font-size: 14px;
    text-align: center;
}

.hint {
    font-size: 14px;
    text-align: center;
    color: #605134;
    margin-top: 10px;
}

.input-num {
    text-align: center;
    font-size: 44px;
    font-family: myfont;
    width: 100%;
    height: 100%;
    border: 0;
    padding: 0;
}

.scan-btn {
    margin: 24rpx auto 0 auto;
    width: 600rpx;
    height: 90rpx;
    line-height: 90rpx;
    background: #ffd900;
    text-align: center;
    color: #000;
    font-family: PingFangSC-Regular;
    background-image: linear-gradient(90deg,#FFEC00 0%,#FECC00 100%);
    box-shadow: 0 20rpx 40rpx 0 rgba(255,221,110,0.60);
    border-radius: 60rpx;
}

.manual-btn {
    font-family: PingFangSC-Regular;
    border: 3rpx solid rgba(68,68,68,0.2);
    border-radius: 60rpx;
    height: 90rpx;
    line-height: 90rpx;
    width: 600rpx;
    font-size: 30rpx;
    color: #2E2121;
    text-align: center;
    margin: 36rpx auto 0 auto;
    background: transparent;
}

.tip {
    font-size: 32rpx;
    color: #333333;
    letter-spacing: 0;
    margin-top: 62rpx;
}

.nu-img-container {
    float: right;
    width: 400rpx;
    height: 360rpx;
}

.nu-img {
    width: 440rpx;
    height: 396rpx;
}

.nu-title {
    font-family: PingFangSC-Semibold;
    font-size: 64rpx;
    color: #333333;
    letter-spacing: 0;
    line-height: 64rpx;
    margin-top: 400rpx;
}

.nu-desc {
    font-family: PingFangSC-Regular;
    font-size: 36rpx;
    color: #666666;
    letter-spacing: 0;
    line-height: 36rpx;
    margin-top: 40rpx;
}

.carno-modal-mask {
    background: rgba(0,0,0,0.5);
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 9999;
}

.carno-modal {
    width: 628rpx;
    height: 568rpx;
    background: #FFFFFF;
    border-radius: 10px;
    text-align: center;
    position: fixed;
    margin: auto;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.carno-close {
    float: right;
    padding-right: 24rpx;
    padding-top: 24rpx;
}

.carno-close .eimg {
    width: 30rpx;
    height: 30rpx;
}

.carno-img {
    padding-top: 60rpx;
    padding-left: 40rpx;
}

.carno-img .eimg {
    width: 220rpx;
    height: 220rpx;
}

.carno-info {
    font-size: 38rpx;
    color: #333333;
    line-height: 38rpx;
    margin-top: 40rpx;
    display: inline-flex;
}

.carno-info .times {
    color: #FC5759;
}

.carno-btn {
    width: 300rpx;
    height: 90rpx;
    background: #FFDC00;
    border-radius: 60px;
    margin: 60rpx auto;
    display: table;
}

.carno-btn .text {
    display: table-cell;
    vertical-align: middle;
}

.carno-btn .detail {
    font-size: 30rpx;
    color: #333333;
    letter-spacing: 0;
}

.carno-btn .tips {
    opacity: 0.6;
    font-size: 24rpx;
    color: #333333;
    letter-spacing: 0;
}

.manual {
    padding-top: 110rpx;
    margin-left: 44rpx;
    background-color: #fff;
}

.manual-title {
    font-family: PingFangSC-Semibold;
    font-size: 64rpx;
    color: #333333;
    letter-spacing: 0;
    line-height: 64rpx;
}

.manual-spec {
    margin-top: 32rpx;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #888888;
    letter-spacing: 0;
    line-height: 32rpx;
}

.manual-blank {
    width: 6rpx;
    height: 36rpx;
    background: #FFDC00;
    display: inline-block;
    position: absolute;
    margin-top: 150rpx;
}

.manual-input {
    border: none;
    border-bottom: solid 3rpx #EAEAEA;
    margin-left: 0!important;
    border-radius: 0;
}

.manual-btn-gap {
    margin-top: 74rpx;
}

input {
    margin-top: 120rpx;
    width: 600rpx;
    height: 90rpx;
    display: inline-block;
}

.input-line {
    background-color: #EAEAEA;
    height: 3rpx;
    width: 662rpx;
    margin: auto;
}

button {
    margin-top: 34rpx;
    background: #CECECE;
    border-radius: 100rpx;
    width: 600rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    margin: 34rpx auto 0 auto;
    border: 0;
    color: #888888;
}

button.active {
    background-image: linear-gradient(90deg,#FFEC00 0%,#FECC00 100%);
    box-shadow: 0 20rpx 40rpx 0 rgba(255,221,110,0.60);
    border-radius: 100rpx;
    color: #333333;
}

.prince-info {
    width: 686rpx;
    margin-top: 30rpx;
    margin-left: 32rpx;
    font-family: PingFangSC-Regular;
    font-size: 25rpx;
    color: #666;
    letter-spacing: 0;
    text-align: center;
    line-height: 39rpx;
}

.image-modal-content {
    width: 630rpx;
    height: 866rpx;
}

.modal-outer-cancel-block {
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    bottom: 120rpx;
}

.modal-outer-cancel-btn {
    width: 60rpx;
    height: 60rpx;
    font-size: 60rpx;
    color: white;
}

.more-btn-block {
    margin-top: 54rpx;
    position: relative;
}

.more-text {
    width: 209rpx;
    height: 30rpx;
    font-size: 26rpx;
    font-family: PingFangSC-Regular;
    color: rgba(255,204,0,1);
    line-height: 30rpx;
}

.more-btn {
    width: 45rpx;
    height: 40rpx;
}

.gift-btn {
    width: 360rpx;
    height: 90rpx;
    background: #FFEC00;
    border-radius: 100rpx;
    text-align: center;
    line-height: 90rpx;
}

.down {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    width: 100vw;
    height: 100vh;
    text-align: center;
}

.down-ofo {
    margin-top: 185rpx;
    width: 382rpx;
    height: 351rpx;
}

.down-app {
    margin-bottom: 128rpx;
}

.down-desc {
    font-size: 28rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(62,58,57,1);
    line-height: 40rpx;
    letter-spacing: 1rpx;
}

.down-ico {
    display: block;
    margin: 11rpx auto 38rpx;
    width: 32rpx;
    height: 32rpx;
}

.stake-container {
    display: flex;
    flex-direction: column;
}

.stake-bottom-box {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}

.stake-nu-img-container {
    float: none;
    width: 100%;
    height: 590rpx;
}

.stake-nu-img {
    width: 100%;
    height: 590rpx;
}

.stake-prince-info {
    display: flex;
    align-items: center;
    margin-top: 0;
    margin-left: 0;
    width: 100%;
    line-height: 64rpx;
    text-align: left;
    background-color: #FFF8DB;
}

.stake-prince-ico {
    display: inline-block;
    margin-left: 32rpx;
    margin-right: 20rpx;
    width: 32rpx;
    height: 32rpx;
}

.stake-prince-info-jump {
    position: absolute;
    right: 38rpx;
}

.stake-box {
    text-align: left;
    font-size: 26rpx;
    padding-left: 36rpx;
    line-height: 51rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(136,136,136,1);
    letter-spacing: .5px;
}

.stake-broke-money {
    font-size: 38rpx;
    color: #FFC100;
    font-weight: bold;
}

.stake-details {
    display: block;
    font-size: 26rpx;
}

.stake-rules {
    margin: 0;
    padding: 0;
    background: transparent;
    margin-top: 30rpx;
    font-size: 26rpx;
    line-height: 1;
    width: 100%;
    height: auto;
    color: #FED87F;
}

.stake-scan-btn {
    margin-top: 0;
}

