.container {
    padding: 0;
}

.map-container {
    box-sizing: border-box;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}

.submit-car {
    width: 100%;
    height: 48px;
    position: absolute;
    bottom: 60px;
    left: 0;
    text-align: center;
    line-height: 48px;
}

.csssprite {
    animation: fadeIn .3s ease-out forwards;
}

@-webkit-keyframes fadeIn {
    from {
        opacity: 0;
        -webkit-transform: translateY(-10px);
    }

    to {
        opacity: 1;
        -webkit-transform: translateY(0px);
    }
}

@keyframes fadeIn {
    from {
        opacity: 0;
        transform: translateY(-10px);
    }

    to {
        opacity: 1;
        transform: translateY(0px);
    }
}

