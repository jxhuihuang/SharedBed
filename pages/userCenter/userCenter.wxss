.container{
    background: #f6f6f6;
}
.userHeader-main{
    height: 120px;
    background: #FFF;
    padding: 0 60rpx;
    display: flex;
    /* justify-content: center; */
    align-items: center
}

.userAvatar{
    flex: 0 0 auto;
    width: 60px;
    height: 60px;
    margin-right: 40rpx;
    border-radius: 50%;
    overflow:hidden;

}

.userAvatar image{
    max-width: 100%;
}

.list-warp{
    background: #FFF;
    min-height: 40px;
    margin-top: 20px;
}

.list-box{
    display: flex;
    min-height: 50px;
    align-items: center;
   
    position: relative;
    /* justify-content: center; */
}
.list-main{
    display: flex;
    align-items: center;
    width: 100%;
}
.showright .list-main{
    width:60%;
}
.list-icon{
    width: 20px;
    
    overflow: hidden;
    margin-right: 20rpx;
    
}
.list-title{
    /* font-family: PingFangSC-Medium; */
    font-size: 32rpx;
}


.list-right{
    width: 40%;
    text-align: right;
    font-size: 12px;
    color: #888;
}

.list-right.icons{
    display: flex;
    position: relative;
    padding-right: 60rpx;
    align-items: center;
}

.list-right .list-link{
    position: absolute;
    width: 45rpx;
    height: 45rpx;
    right: 15rpx;
    top: 0;
    bottom: 0;
    margin: auto;
}

.userIcon{
    color: #666;
    font-size: 34rpx;
}
.userIcon.cardRoll-icon{
    font-size: 26rpx;
}
.userIcon.wallet-icon{
    font-size: 30rpx;
}